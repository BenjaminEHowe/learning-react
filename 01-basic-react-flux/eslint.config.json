{
    "root": true,
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 1
    },
    "globals": {
        "jQuery": true,
        "$": true
    }
}